{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "A document management system that defines roles, privileges and access rights of users",
  "main": "index.js",
  "scripts": {
    "start": "node tools/distServer",
    "dev:start": "babel-node tools/startLog.js",
    "dev": "npm-run-all dev:start open:src",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* client tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "babel-node node_modules/babel-istanbul/lib/cli cover _mocha -- --require ./server/test/testSetup.js --recursive ./server/test -R spec",
    "test:watch": "npm run test -- --watch",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "test:frontend": "mocha --compilers js:babel-register --require ./client/tests/helpers.js --require ./client/tests/dom.js --recursive",
    "tdd": "npm run test:frontend -- --watch",
    "test-e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "clean-dist": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist build:html",
    "build:html": "babel-node tools/buildHtml.js",
    "build": "babel-node tools/build.js",
    "postinstall": "node nightwatch.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-oaboluwarin/document-management-system.git"
  },
  "keywords": [
    "babel",
    "document",
    "management",
    "eslint"
  ],
  "author": "oreoluwa aboluwarin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-oaboluwarin/document-management-system/issues"
  },
  "homepage": "https://github.com/andela-oaboluwarin/document-management-system#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.22.0",
