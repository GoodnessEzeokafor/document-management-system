    "eslint-plugin-react": "7.17.0",
    "eventsource-polyfill": "0.9.6",
    "expect": "24.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "faker": "^4.1.0",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jsdom": "15.2.1",
    "mocha": "^6.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "react-addons-test-utils": "^15.0.2",
